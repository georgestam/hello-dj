
<p>Logged in as: <%= current_user.email %></p>
<h3>Your Bookings:</h3>
<% current_user.bookings.each do |booking| %>
Date:<%= booking.date %>
Time:<%= booking.time %>
DJ:<%= booking.profile.dj_name %>
<br>
<% end %>




<% if current_user.profiles %>
  <h3>DJ Profiles you have created:</h3>
  <% current_user.profiles.each do |profile| %>
  DJ Name:<%= profile.dj_name %>
  <% end %>
<% else %>
  <p>You are currently not regestered as a DJ.<p>
  <%= link_to "Create DJ Profile", new_profile_path  , :class => "btn btn-danger" %>
<% end %>



